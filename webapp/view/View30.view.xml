<Page id="_IDGenPage" title="Calificación de Productos">
    <List id="_IDGenList"
      items="{/products}"
      headerText="Selecciona una calificación">
      <items>
        <CustomListItem id="_IDGenCustomListItem">
          <HBox id="_IDGenHBox1" justifyContent="SpaceBetween" alignItems="Center">
            <Text id="_IDGenText7" text="{name}" />
            <RatingIndicator id="_IDGenRatingIndicator"
              value="{rating}"
              maxValue="5"
              liveChange="onRatingChange" />
          </HBox>
        </CustomListItem>
      </items>

 </List>
  </Page>

