<mvc:View controllerName="ejercicio15.project1.controller.View1"
    xmlns:dnd="sap.ui.core.dnd"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:core="sap.ui.core"
    xmlns="sap.m">

    <Page id="page">
        <content>
            <HBox id="_IDGenHBox" renderType="Bare">
                <mvc:XMLView id="availableProducts" viewName="ejercicio15.project1.view.AvailableProducts" async="true"/>
                <VBox id="_IDGenVBox" justifyContent="Center" class="sapUiTinyMarginBeginEnd">
                    <Button id="_IDGenButton" icon="sap-icon://navigation-right-arrow" tooltip="Move to selected" press="moveToSelectedProductsTable"/>
                    <Button id="_IDGenButton1" icon="sap-icon://navigation-left-arrow" tooltip="Move to available" press="moveToAvailableProductsTable"/>
                </VBox>
                <mvc:XMLView id="selectedProducts" viewName="ejercicio15.project1.view.SelectedProducts" async="true"/>
            </HBox>
        </content>

    <Button id="botonView21" type="Accept"
						text="Tarjeta de producto"
						press="onGoView21">
						
					<layoutData>
						<FlexItemData id="_IDGenFlexItemData2" growFactor="1" />
					</layoutData>
				</Button>

    <Button id="botonView23" type="Accept"
						text="Lista de tareas"
						press="onGoView23">
						
					<layoutData>
						<FlexItemData id="_IDGenFlexItemData3" growFactor="1" />
					</layoutData>
				</Button>


    <Button id="botonView24" type="Accept"
						text="BusyIndicator"
						press="onGoView24">
						
					<layoutData>
						<FlexItemData id="_IDGenFlexItemData4" growFactor="1" />
					</layoutData>
				</Button>


    <Button id="botonView25" type="Accept"
						text="ObjectPageLayout"
						press="onGoView25">
						
					<layoutData>
						<FlexItemData id="_IDGenFlexItemData5" growFactor="1" />
					</layoutData>
				</Button>

	<Button id="botonView26" type="Accept"
						text="Aplicacion responsive"
						press="onGoView26">
						
					<layoutData>
						<FlexItemData id="_IDGenFlexItemData6" growFactor="1" />
					</layoutData>
				</Button>

    <Button id="botonView27" type="Accept"
						text="Modo claro/oscuro"
						press="onGoView27">
						
					<layoutData>
						<FlexItemData id="_IDGenFlexItemData7" growFactor="1" />
					</layoutData>
				</Button>

	<Button id="botonView28" type="Accept"
						text="Vista con un ComboBox"
						press="onGoView28">
						
					<layoutData>
						<FlexItemData id="_IDGenFlexItemData8" growFactor="1" />
					</layoutData>
				</Button>

	<Button id="botonView29" type="Accept"
						text="Tabla elección múltiple y botón eliminar elementos seleccionados."
						press="onGoView29">
						
					<layoutData>
						<FlexItemData id="_IDGenFlexItemData9" growFactor="1" />
					</layoutData>
				</Button>

	<Button id="botonView30" type="Accept"
						text="Calificar productos con estrellas"
						press="onGoView30">
						
					<layoutData>
						<FlexItemData id="_IDGenFlexItemData10" growFactor="1" />
					</layoutData>
				</Button>

    </Page>
</mvc:View>