<mvc:View controllerName="ejercicio15.project1.controller.View1"
    xmlns:dnd="sap.ui.core.dnd"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:core="sap.ui.core"
    xmlns="sap.m">

    <Page id="page">
        <content>
            <HBox id="_IDGenHBox" renderType="Bare">
                <mvc:XMLView id="availableProducts" viewName="ejercicio15.project1.view.AvailableProducts" async="true"/>
                <VBox id="_IDGenVBox" justifyContent="Center" class="sapUiTinyMarginBeginEnd">
                    <Button id="_IDGenButton" icon="sap-icon://navigation-right-arrow" tooltip="Move to selected" press="moveToSelectedProductsTable"/>
                    <Button id="_IDGenButton1" icon="sap-icon://navigation-left-arrow" tooltip="Move to available" press="moveToAvailableProductsTable"/>
                </VBox>
                <mvc:XMLView id="selectedProducts" viewName="ejercicio15.project1.view.SelectedProducts" async="true"/>
            </HBox>
        </content>

    <Button id="botonView21" type="Accept"
						text="Tarjeta de producto"
						press="onGoView21">
						
					<layoutData>
						<FlexItemData id="_IDGenFlexItemData2" growFactor="1" />
					</layoutData>
				</Button>

    <Button id="botonView23" type="Accept"
						text="Lista de tareas"
						press="onGoView23">
						
					<layoutData>
						<FlexItemData id="_IDGenFlexItemData3" growFactor="1" />
					</layoutData>
				</Button>


    <Button id="botonView24" type="Accept"
						text="BusyIndicator"
						press="onGoView24">
						
					<layoutData>
						<FlexItemData id="_IDGenFlexItemData4" growFactor="1" />
					</layoutData>
				</Button>

    </Page>
    
</mvc:View>